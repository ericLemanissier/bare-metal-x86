{
    "version": "2.0.0",
    "tasks": [{
        "label": "Launch QEMU",
        "type": "shell",
        "isBackground": true,
        "windows": {
            "command": "qemu-system-i386 -S -gdb tcp::1234 -boot d -cdrom bin/myos.iso -m 512"
        },
        "linux": {
            "command": "qemu-system-i386 -S -gdb tcp::1234 -cdrom myos.iso -serial stdio"
        }
    }]
}
